<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="AnchorPane" prefHeight="382.0" prefWidth="399.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.yedam.account.LoginController">
   <children>
      <GridPane layoutX="62.0" layoutY="45.0" prefHeight="200.0" prefWidth="275.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="75.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="131.0" minHeight="10.0" prefHeight="131.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="txtUsername" prefHeight="36.0" prefWidth="275.0" promptText="username or email" />
            <PasswordField fx:id="txtPassword" prefHeight="36.0" prefWidth="275.0" promptText="password" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <HBox alignment="CENTER" layoutX="34.0" layoutY="245.0" prefHeight="100.0" prefWidth="331.0" spacing="50.0">
         <children>
            <Button fx:id="btnSignup" onMouseClicked="#handleButtonAction" mnemonicParsing="false" prefHeight="53.0" prefWidth="140.0" text="Sign up" />
            <Button fx:id="btnSignin" onMouseClicked="#handleButtonAction" mnemonicParsing="false" prefHeight="53.0" prefWidth="140.0" text="Sing in" />
         </children>
      </HBox>
   </children>
</AnchorPane>
